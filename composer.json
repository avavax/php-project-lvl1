{
    "name": "avavax/brain-games",
    "type": "library",
    "description": "brain-games package",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "avavax",
            "email": "ip-media@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "BrainGames\\Cli\\": "src",
            "BrainGames\\Even\\": "src"
        },
        "files": [
            "src/cli.php",
            "src/engine.php",
            "src/games/even.php",
            "src/games/calc.php",
            "src/games/gcd.php"
        ]
    },
    "scripts": {
        "phpcs": "phpcs"
    },
    "require": {
        "wp-cli/php-cli-tools": "^0.11.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "bin": [
        "./brain-games",
        "./brain-even",
        "./brain-calc",
        "./brain-gcd"
    ]
}
